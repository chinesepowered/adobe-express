{
  "id": "accessibility-validator-addon",
  "name": "Accessibility Validator",
  "version": "1.0.0",
  "manifestVersion": 2,
  "requirements": {
    "apps": [
      {
        "name": "Express",
        "apiVersion": 1
      }
    ]
  },
  "entryPoints": [
    {
      "type": "panel",
      "id": "accessibility-panel",
      "label": "Accessibility Validator"
    }
  ],
  "uiEntry": {
    "type": "panel",
    "src": "index.html",
    "size": {
      "width": 320,
      "height": 600
    }
  },
  "permissions": {
    "webview": {
      "allow": "yes",
      "domains": ["https://localhost:5241", "http://localhost:5241"]
    },
    "localFileAccess": "no",
    "launchProcess": {
      "schemes": [],
      "extensions": []
    }
  }
}